{
  "private": true,
  "name": "studenten-bilden-schueler.de",
  "title": "Studenten bilden Schüler",
  "description": "Gatsby Site powering the homepage and blog of the German student-run non-profit organisation Studenten bilden Schüler e.V.",
  "homepage": "https://studenten-bilden-schueler.de",
  "version": "1.0.0",
  "author": "Janosh Riebesell <janosh.riebesell@studenten-bilden-schueler.de>",
  "repository": "https://github.com/StudentenBildenSchueler/sbs-homepage",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "2.17.4",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-algolia": "^0.3.4",
    "gatsby-plugin-catch-links": "2.1.15",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-manifest": "2.2.23",
    "gatsby-plugin-netlify": "2.1.22",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "3.0.16",
    "gatsby-plugin-react-helmet": "3.1.13",
    "gatsby-plugin-sharp": "2.2.32",
    "gatsby-plugin-styled-components": "3.1.11",
    "gatsby-remark-embed-video": "^2.0.1",
    "gatsby-remark-images-contentful": "2.1.20",
    "gatsby-remark-responsive-iframe": "2.2.25",
    "gatsby-remark-smartypants": "2.1.14",
    "gatsby-source-contentful": "2.1.50",
    "gatsby-transformer-remark": "2.6.30",
    "gatsby-transformer-sharp": "^2.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^6.0.0-beta",
    "react-instantsearch-dom": "^6.0.0",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1",
    "sharp": "0.23.1",
    "styled-components": "^4.4.0",
    "styled-icons": "^8.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "contentful-management": "^5.11.3",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "dev": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve -o -H 0.0.0.0",
    "deploy": "gatsby build && netlify deploy",
    "clean": "gatsby clean && gatsby build && gatsby serve -o",
    "debugDev": "node --inspect node_modules/.bin/gatsby develop -o -H 0.0.0.0",
    "debugProd": "node --inspect node_modules/.bin/gatsby build"
  },
  "keywords": [
    "Studenten bilden Schüler e.V.",
    "German student-run nonprofit",
    "education",
    "nonprofit",
    "equal opportunities",
    "gatsby",
    "styled-components",
    "Algolia search",
    "fluid typography",
    "CSS grid"
  ],
  "eslintIgnore": [
    "node_modules",
    ".cache",
    "public"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 85,
    "arrowParens": "avoid"
  }
}
