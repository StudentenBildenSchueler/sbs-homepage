  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "2.15.28",
    "gatsby-image": "^2.2.23",
    "gatsby-plugin-algolia": "^0.3.4",
    "gatsby-plugin-catch-links": "2.1.12",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-manifest": "2.2.20",
    "gatsby-plugin-netlify": "2.1.17",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "3.0.11",
    "gatsby-plugin-react-helmet": "3.1.10",
    "gatsby-plugin-sharp": "2.2.27",
    "gatsby-plugin-styled-components": "3.1.8",
    "gatsby-remark-embed-video": "^2.0.0",
    "gatsby-remark-images-contentful": "2.1.17",
    "gatsby-remark-responsive-iframe": "2.2.20",
    "gatsby-remark-smartypants": "2.1.11",
    "gatsby-source-contentful": "2.1.44",
    "gatsby-transformer-remark": "2.6.26",
    "gatsby-transformer-sharp": "^2.2.19",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-headroom": "^2.2.8",
    "react-helmet": "^6.0.0-beta",
    "react-instantsearch-dom": "^5.7.0",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^4.4.0",
    "styled-icons": "^8.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "contentful-management": "^5.11.2",
    "eslint": "^6.4.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1"
  },
  "scripts": {
    "dev": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve -o -H 0.0.0.0",
    "deploy": "gatsby build && netlify deploy",
    "clean": "gatsby clean && gatsby build && gatsby serve -o",
    "debugDev": "node --inspect node_modules/.bin/gatsby develop -o -H 0.0.0.0",
    "debugProd": "node --inspect node_modules/.bin/gatsby build"
  },
  "keywords": [
    "Studenten bilden Schüler e.V.",
    "German student-run nonprofit",
    "education",
    "nonprofit",
    "equal opportunities",
    "gatsby",
    "styled-components",
    "Algolia search",
    "fluid typography",
    "CSS grid"
  ],
  "eslintIgnore": [
    "node_modules",
    ".cache",
    "public"
  ]
}
