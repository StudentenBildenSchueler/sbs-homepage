{
  "private": true,
  "name": "studenten-bilden-schueler.de",
  "title": "Studenten bilden Schüler",
  "description": "Gatsby Site powering the homepage and blog of the German student-run non-profit organisation Studenten bilden Schüler e.V.",
  "homepage": "https://studenten-bilden-schueler.de",
  "version": "1.0.0",
  "author": "Janosh Riebesell <janosh.riebesell@studenten-bilden-schueler.de>",
  "repository": "https://github.com/StudentenBildenSchueler/sbs-homepage",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "2.19.49",
    "gatsby-image": "^2.2.45",
    "gatsby-plugin-algolia": "^0.5.0",
    "gatsby-plugin-catch-links": "2.1.28",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-manifest": "2.2.48",
    "gatsby-plugin-netlify": "2.1.35",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "3.0.42",
    "gatsby-plugin-react-helmet": "3.1.24",
    "gatsby-plugin-sharp": "2.4.13",
    "gatsby-plugin-styled-components": "3.1.21",
    "gatsby-remark-autolink-headers": "^2.1.26",
    "gatsby-remark-embed-video": "^2.0.1",
    "gatsby-remark-images-contentful": "2.1.35",
    "gatsby-remark-responsive-iframe": "2.2.34",
    "gatsby-remark-smartypants": "2.1.23",
    "gatsby-source-contentful": "2.1.100",
    "gatsby-transformer-remark": "2.6.59",
    "gatsby-transformer-sharp": "^2.3.19",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0-beta.2",
    "react-instantsearch-dom": "^6.4.0",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1",
    "sharp": "0.25.1",
    "styled-components": "^5.0.1",
    "styled-icons": "^9.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "contentful-management": "^5.13.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "dev": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve -o -H 0.0.0.0",
    "deploy": "gatsby build && netlify deploy",
    "clean": "gatsby clean && gatsby build && gatsby serve -o",
    "debugDev": "node --inspect node_modules/.bin/gatsby develop -o -H 0.0.0.0",
    "debugProd": "node --inspect node_modules/.bin/gatsby build"
  },
  "keywords": [
    "Studenten bilden Schüler e.V.",
    "German student-run nonprofit",
    "education",
    "nonprofit",
    "equal opportunities",
    "gatsby",
    "styled-components",
    "Algolia search",
    "fluid typography",
    "CSS grid"
  ],
  "eslintIgnore": [
    "node_modules",
    ".cache",
    "public"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 85,
    "arrowParens": "avoid"
  }
}
