{
  "private": true,
  "name": "studenten-bilden-schueler.de",
  "title": "Studenten bilden Schüler",
  "description": "Gatsby Site powering the homepage and blog of the German student-run non-profit organisation Studenten bilden Schüler e.V.",
  "homepage": "https://studenten-bilden-schueler.de",
  "version": "1.0.0",
  "author": "Janosh Riebesell <janosh.riebesell@studenten-bilden-schueler.de>",
  "repository": "https://github.com/StudentenBildenSchueler/sbs-homepage",
  "dependencies": {
    "airtable": "^0.8.1",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "2.22.9",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-algolia": "^0.11.0",
    "gatsby-plugin-catch-links": "2.3.2",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-manifest": "2.4.9",
    "gatsby-plugin-netlify": "2.3.3",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "3.2.7",
    "gatsby-plugin-react-helmet": "3.3.2",
    "gatsby-plugin-sharp": "2.6.9",
    "gatsby-plugin-styled-components": "3.3.2",
    "gatsby-remark-attr": "^0.0.3",
    "gatsby-remark-autolink-headers": "^2.3.3",
    "gatsby-remark-embed-video": "^3.0.10",
    "gatsby-remark-emoji": "^0.0.3",
    "gatsby-remark-images-contentful": "2.3.2",
    "gatsby-remark-responsive-iframe": "2.4.3",
    "gatsby-remark-smartypants": "2.3.2",
    "gatsby-source-contentful": "2.3.12",
    "gatsby-transformer-remark": "2.8.13",
    "gatsby-transformer-sharp": "^2.5.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-hook-form": "^5.7.2",
    "react-instantsearch-dom": "^6.5.0",
    "react-select": "^3.1.0",
    "react-spring": "^8.0.27",
    "sharp": "0.25.3",
    "styled-components": "^5.1.0",
    "styled-icons": "^10.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "contentful-management": "^5.26.3",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "dev": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve -o -H 0.0.0.0",
    "deploy": "gatsby build && netlify deploy",
    "clean": "gatsby clean && gatsby build && gatsby serve -o",
    "debugDev": "node --inspect node_modules/.bin/gatsby develop -o -H 0.0.0.0",
    "debugProd": "node --inspect node_modules/.bin/gatsby build"
  },
  "keywords": [
    "Studenten bilden Schüler e.V.",
    "German student-run nonprofit",
    "education",
    "nonprofit",
    "equal opportunities",
    "gatsby",
    "styled-components",
    "Algolia search",
    "fluid typography",
    "CSS grid"
  ],
  "eslintIgnore": [
    "node_modules",
    ".cache",
    "public"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "printWidth": 85,
    "arrowParens": "avoid"
  }
}
